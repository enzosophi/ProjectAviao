<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white_2">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:id="@+id/vensco_logo"
            android:src="@drawable/iconvesnco"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginStart="24sp"
            android:text="Informações da viagem"
            android:textColor="@color/fourth_color"
            android:textSize="24dp"
            android:textStyle="bold"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/vensco_logo" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24sp"
            android:layout_marginTop="15dp"
            android:text="Partida"
            app:layout_constraintTop_toBottomOf="@id/info"
            app:layout_constraintStart_toStartOf="parent"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/partida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/textview_style"
            android:paddingStart="12sp"
            app:layout_constraintTop_toBottomOf="@id/textView"
            android:paddingTop="3dp"
            android:text="TextView"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24sp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/partida"
            app:layout_constraintStart_toStartOf="parent"
            android:text="Destino"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/destino"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/textview_style"
            android:paddingStart="12sp"
            app:layout_constraintTop_toBottomOf="@id/textView2"
            android:paddingTop="3dp"
            android:text="TextView"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24sp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/destino"
            app:layout_constraintStart_toStartOf="parent"
            android:text="Data"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/Data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/textview_style"
            android:paddingStart="12sp"
            app:layout_constraintTop_toBottomOf="@id/textView3"
            android:paddingTop="3dp"
            android:text="TextView"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24sp"
            android:layout_marginTop="10dp"
            android:text="Horario"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/Data"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/Horario"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/textview_style"
            android:paddingStart="12sp"
            app:layout_constraintTop_toBottomOf="@id/textView4"
            android:paddingTop="3dp"
            android:text="TextView"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24sp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/Horario"
            android:text="Aeronave"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/Aeronave"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/textview_style"
            app:layout_constraintTop_toBottomOf="@id/textView5"
            android:paddingStart="12sp"
            android:paddingTop="3dp"
            android:text="TextView"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24sp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/Aeronave"
            android:text="Passageiros"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/Passageiros"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/textview_style"
            app:layout_constraintTop_toBottomOf="@id/textView6"
            android:paddingStart="12sp"
            android:paddingTop="3dp"
            android:text="TextView"
            android:textSize="20sp" />


        <Button
            android:id="@+id/button_passager"
            android:layout_width="235dp"
            android:layout_height="40sp"
            android:layout_marginTop="15dp"
            android:backgroundTint="@color/fourth_color"
            android:text="Passageiros"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.8"
            app:layout_constraintTop_toBottomOf="@id/Passageiros" />


        <FrameLayout
            android:id="@+id/frameLayout_items"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent">

                <com.google.android.material.bottomnavigation.BottomNavigationView
                    android:id="@+id/bottomNavigation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:menu="@menu/nav_menu_botton"
                    app:itemIconTint="@color/fourth_color"/>
        </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>